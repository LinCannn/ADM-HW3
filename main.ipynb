    "    words = row['description_filtered'].split()  # Tokenize \n",
    "    word_counts = Counter(words)\n",
    "    total_terms = len(words)\n",
    "    tf[doc_id] = {word: count / total_terms for word, count in word_counts.items()}"
   ]
